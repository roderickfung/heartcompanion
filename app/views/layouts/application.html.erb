<!DOCTYPE html>
<html>
  <head>
    <title>Heartapp</title>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag "https://www.google.com/jsapi" %>
    <script src="//maps.google.com/maps/api/js?v=3.23&key=AIzaSyCy1NBsfYySEaaNN8c64ZUa66hK4NJSuaE"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body class='landing'>
    <%= render partial: '/partials/top_nav' %>
    <div class='container'>
      <div class = 'page-space-lg'></div>
      <div class='alert-success' style='z-index: 1;'>
        <center><%=flash[:notice]%></center>
      </div>
      <div class='alert-danger' style='z-index: 1;'>
        <center><%=flash[:alert]%></center>
      </div>
    </div>
    <%= yield %>
    <%= render partial: '/modals/pat_login' %>
    <%= render partial: '/modals/clin_login' %>
    <div class='footer'>
      <div class='container'>
        <%= render partial: '/layouts/footer'%>
      </div>
    </div>
  </body>
</html>
